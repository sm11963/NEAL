rule Bang {
  Swift::ForcedValueExpression {
    fail("No force unwrap")
  }

  tests {
    test1: expect_ok("x")
    test2: expect_fail("x!")
  }
}

rule Print {
  Swift::CallExpression where Callee == "print" {
    warn("Don't call `print`")
  }

  tests {
    test1: expect_ok("printf()")
    test2: expect_warn("print()")
  }
}

rule NSLog {
  Swift::CallExpression where Callee == "NSLog" {
    warn("Don't call `NSLog`")
  }

  tests {
    test1: expect_ok("print(\"astr\")")
    test2: expect_warn("NSLog(\"astr\")")
  }
}
